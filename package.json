    "vue-template-compiler": "^2.6.11",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^2.0.6",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
